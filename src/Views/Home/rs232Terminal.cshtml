
@{
    ViewBag.Title = "rs232Terminal";
    ViewBag.Meta = "تست پورت سریال نشاندهنده باسکول";
    Layout = "~/Views/Shared/ExampleLayout2.cshtml";
}

   

<html>
<head>
    <meta http-equiv=origin-trial content=AuousLhuxAkHb7jnvhL1wBR8x+uFyTWFYmZMA2R0TB2oWWOjQrcA85qf0PErafuccNzWKekf2P3h0MoLf9ykbggAAABaeyJvcmlnaW4iOiJodHRwczovL2dvb2dsZWNocm9tZWxhYnMuZ2l0aHViLmlvOjQ0MyIsImZlYXR1cmUiOiJTZXJpYWwiLCJleHBpcnkiOjE2MTM1MTE2MjZ9>
    <meta http-equiv=origin-trial content="An0ljMvC1hXs0YiXduDfUeFET2BDmYLxFsbNvDTxJHuEn6uNJHKmkvfuPq/ZY4gtPo0ilxKzplB1fmvr77SHFAwAAABJeyJvcmlnaW4iOiJodHRwOi8vbG9jYWxob3N0OjgwODAiLCJmZWF0dXJlIjoiU2VyaWFsIiwiZXhwaXJ5IjoxNjEzNTExNjY5fQ==">
    <style>
        body {
            padding: 0;
            margin: 0
        }

        * {
            box-sizing: border-box
        }

        #terminal {
            height: 90vh
        }

        .options {
            height: 10vh;
            padding: 15px
        }
    </style>
    <style type="text/css">
        @@font-face {
            font-family: 'YekanIzady';
            src: url('../../fonts/BYekan.ttf') format('truetype');
        }

        .classname {
            font-family: 'YekanIzady';
        }
    </style>
</head>
<body>
    <div>
        <h1 style="font-family:YekanIzady;">تست پورت سریال نشاندهنده باسکول</h1>
    </div>
    <div class=options>
        <label for=ports>Port:</label> <select id=ports>
            <option value=prompt>Add a port...</option>
        </select>
        <button id=connect>Connect</button>
        <label for=baudrate>Baud rate:</label>
        <select id=baudrate>
            <option value=9600>9600</option>
            <option value=14400>14400</option>
            <option value=19200>19220</option>
            <option value=28800>28800</option>
            <option value=38400>38400</option>
            <option value=57600>57600</option>
            <option value=115200 selected=selected>115200</option>
            <option value=custom>Custom</option>
        </select> <br>
        <input id=custom_baudrate type=number min=1 placeholder="Enter baudrate..." hidden>
        <label for=databits>Data bits:</label>
        <select id=databits>
            <option value=7>7</option>
            <option value=8 selected=selected>8</option>
        </select>
        <label for=parity>Parity:</label>
        <select id=parity>
            <option value=none selected=selected>None</option>
            <option value=even>Even</option>
            <option value=odd>Odd</option>
        </select>
        <label for=stopbits>Stop bits:</label>
        <select id=stopbits>
            <option value=1 selected=selected>1</option>
            <option value=2>2</option>
        </select> <input id=rtscts type=checkbox>
        <label for=rtscts>Hardware flow control</label>
        <input id=echo type=checkbox>
        <label for=echo>Local echo</label>
        <input id=enter_flush type=checkbox>
        <label for=enter_flush>Flush on enter</label>
        <button id=download>Download output</button>
    </div> <div id=terminal></div>
    <h5>chrome://flags/#enable-experimental-web-platform-features</h5>
    <script type="text/javascript" src="~/Scripts/Izady/main.5cd8e059b41f535b4695.js"></script>
    <script type="text/javascript" src="~/Scripts/Izady/runtime.58908f3dbdb804a00215.js"></script>
    <script type="text/javascript" src="~/Scripts/Izady/vendors.71b54d27fa1b1e8bdfa6.js"></script>   
</body>
</html> 